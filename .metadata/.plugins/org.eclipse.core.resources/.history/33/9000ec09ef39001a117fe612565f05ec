
public class ArraysPrimitiveDataTypes {


	public static void main(String[] args) {
		
		int[] arrays; //Declare array with type int
		//int arrays2[]; - same thought as above, just a different type of noting it down
		arrays = new int[2];
		
		int[] arr = new int[10];
		
		int[] arrTest = new int[5];
		
		arrTest[0] = 10;
		arrTest[1] = 3;
		arrTest[4] = 4;
		
		System.out.println(arrTest[0] + " " +arrTest[1]);
		

	}

}
