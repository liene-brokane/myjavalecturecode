
public class PrimType {

	public static void main(String[] args) {
		//Primitive numeric data type
		int integer = 2;
		long longInteger = 20204040240L;
		short shortInteger = 2020;
		byte byteInteger = 120;
		//Floating-Point type real numbers
		double doubleFloatNumber = 0.5;
		float floatNumber = 0.12412412f;
		//Characters
		char charSymbol = 'a';
		char charSymbolAsNumber = 33;
		System.out.println("Char: "+charSymbolAsNumber);
		char charFirst = 76;
		char charSecond = 105;
		char charThird = 101;
		char charFourth = 110;
		System.out.println("VƒÅrds: "+charFirst+charSecond+charThird+charFourth+charThird);
		//Booleans
		boolean booleanTrueFalse= true;
		System.out.println(booleanTrueFalse);
	}

}
