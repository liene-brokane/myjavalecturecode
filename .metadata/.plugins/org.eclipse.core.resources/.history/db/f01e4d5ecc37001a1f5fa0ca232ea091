
public class TaskVar {

	public static void main(String[] args) {
		int numAdd;
		numAdd = 125 + 24;
		System.out.println(numAdd + " = 125 + 24");
		int numDel;
		numDel = 125 - 24;
		System.out.println(numDel + " = 125 - 24");
		int numMult;
		numMult = 125 x 24;
		System.out.println(numMult + " = 125 x 24");
	}

}
