
public class TestIfElseAndBlocks {


	public static void main(String[] args) {
		System.out.println("*******");
		System.out.println("*******");
		
		boolean test = true;
		
		if(test){
			System.out.println("TRUE");
		}else{
			System.out.println("FALSE");
		}
		System.out.println("AFTER if else block");
		
		
		boolean test2 = false;
		
		if(test2){
			System.out.println("TRUE");
		}else{
			System.out.println("FALSE");
		}
		System.out.println("AFTER if else block");
	}

}
