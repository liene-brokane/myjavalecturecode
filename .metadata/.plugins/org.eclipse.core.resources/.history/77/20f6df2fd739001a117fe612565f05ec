
public class Calculator {


	public static void main(String[] args) {
		//Operators to use: +,-,/,*,%
		//char c = '/';
		//if(c=='/'){syso(num1/num2}else if(c=='*'){syso(num1*num2}etc.
		
		char operatorMath = '/';
		
		double num1 = 30.0;
		double num2 = 24.5;
		double result;
		
		if(operatorMath == '+'){
			result = num1 + num2;
			System.out.println(result);
		}else if(operatorMath == '-'){
			result = num1 - num2;
			System.out.println(result);
		}else if(operatorMath == '*'){
			result = num1 * num2;
			System.out.println(result);
		}else if(operatorMath == '/'){
			result = num1 / num2;
			System.out.println(result);
		}else if(operatorMath == '%'){
			result = num1 % num2;
			System.out.println(result);
		}else{
			System.out.println("Wrong data");
		}
	}
}