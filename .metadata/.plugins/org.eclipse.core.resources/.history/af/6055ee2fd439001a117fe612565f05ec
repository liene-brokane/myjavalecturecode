
public class Calculator {


	public static void main(String[] args) {
		//Operators to use: +,-,/,*,%
		//char c = '/';
		//if(c=='/'){syso(num1/num2}else if(c=='*'){syso(num1*num2}
		
		char operatorMath = '+';
		
		double num1 = 20.0;
		double num2 = 24.5;
		
		if(operatorMath == '+'){
			System.out.println(num1 + num2);
		}else if(operatorMath == '-'){
			System.out.println(num1 - num2);
		}else if(operatorMath == '*'){
			System.out.println(num1 * num2);
		}else if(operatorMath == '/'){
			System.out.println(num1 / num2);
		}else if(operatorMath == '%'){
			System.out.println(num1 % num2);
		
		}
	}
}